      </div>
      <div>
         <label for="age">Idade</label>
         <input type="text" id="age" name="activity[age]">
      </div>
      <div>
         <label for="sensiblePeriod">Período Sensível</label>
         <input type="text" id="sensiblePeriod" name="activity[sensiblePeriod]">
      </div>
      <div>
         <label for="category">Categoria</label>
         <input type="text" id="category" name="activity[category]">
      </div>
      <div>
         <label for="theme">Tema</label>
         <input type="text" id="theme" name="activity[theme]">
      </div>
      <div>
         <label for="description">Descrição</label>
         <input type="text" id="description" name="activity[description]">
